{
  "languageserver": {
    "graphql": {
      "enable": true,
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": ["typescript", "typescriptreact", "graphql"]
    },
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    }
  },
  "diagnostic.errorSign": "âœ˜",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextLines": 1,
  "diagnostic.virtualTextPrefix": " ",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "coc.preferences.enableMessageDialog": true,
  "codeLens.enable": true,
  "typescript.implementationsCodeLens.enable": true,
  "typescript.referencesCodeLens.enable": true,
  "diagnostic.messageTarget": "float",
  "coc.preferences.rootPatterns": [
    ".editorconfig",
    "node_modules",
    "tsconfig.json",
    ".git"
  ],
  "javascript.validate.enable": false,
  "prettier.semi": true,
  "prettier.tslintIntegration": true,
  "prettier.eslintIntegration": true,
  "eslint.autoFixOnSave": true,
  "coc.preferences.floatActions": false,
  "coc.preferences.silentAutoupdate": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.useQuickfixForLocations": true,
  "cSpellExt.enableDictionaries": ["portuguese"],
  "cSpell.language": "en,pt",
  "cSpell.showCommandsInEditorContextMenu": false,
  "cSpell.userWords": [
    "Collatable",
    "DSVs",
    "DTCs",
    "Deduplication",
    "Finalise",
    "Formik",
    "Fulfilment",
    "Gvdiffsplit",
    "IFORCE",
    "Loggi",
    "OWNCAR",
    "Postgraphile",
    "Redrive",
    "SKUs",
    "TESTCONTAINERS",
    "UKMAIL",
    "Unbuyability",
    "Unprocessable",
    "Wrapabble",
    "Wrappable",
    "algoliasearch",
    "branchn",
    "browserslist",
    "bujo",
    "buyability",
    "comms",
    "consts",
    "deliverypromise",
    "dryrun",
    "dundev",
    "dunelm",
    "dunlocal",
    "dunprod",
    "dunqa",
    "fastify",
    "graphile",
    "initialise",
    "lockdown",
    "pactfile",
    "pgrep",
    "pgsql",
    "precommit",
    "reqheaders",
    "require'init",
    "retryable",
    "stringlist",
    "testcafe",
    "unbuyable",
    "upsert",
    "worldstores",
    "wsrun"
  ],
  "cSpell.enabledLanguageIds": [
        "csharp",
        "go",
        "javascript",
        "javascriptreact",
        "markdown",
        "php",
        "plaintext",
        "typescript",
        "typescriptreact",
        "yml",
        "vimwiki"
    ]
}
